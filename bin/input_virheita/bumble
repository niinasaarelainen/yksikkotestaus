<a1,f1,d1> g#1 g1 f#1 <g1,d1> f#1 f1 e1    <f1,d1> f#1 g1 g#1 <a1,g1,c#1> b1 h1 c2 

#pääteema transponoituna:
<d2,b1,g1> c#2 c2 h1 b1 eb2 d2 c#2    <d2,b1,g1> c#2 c2 h1 <b1,g1,e1> h1 c2 c#2 
<d2,b1,g1> c#2 c2 h1 b1 eb2 d2 c#2    <d2,b1,g1> c#2 c2 h1 <b1,g1,e1> h1 c2 c#2 

<d2,b1,g1> c#2 c2 h1  <c2,g1,eb1> h1 b1 a1     <b1,g1,d1> h1 c2 c#2    <d2,c2,f#1> eb2 d2 c#2
<d2,b1,g1> c#2 c2 h1  <c2,g1,eb1> h1 b1 a1     <b1,g1,d1> h1 c2 c#2    <d2,c2,f#1> eb2 d2 c#2

#vasemman soolo:
<d2,d1> eb1 d1 eb1 d1 eb1 d1 eb1   <d1,c#2,eb2> eb1 d1 eb1 d1 eb1 d1 eb1
<d2,d1> eb1 d1 eb1 d1 eb1 d1 eb1   <d1,c#2,eb2> eb1 d1 eb1 d1 eb1 d1 eb1

<d2,d1> eb1 d1 eb1 <d1,c#2,eb2> eb1 d1 eb1     <d2,d1> eb1 <d1,c#2,eb2> eb1 <d1,c2,e2> eb1 <d1,f2,h1> eb1
<d1,f#2,d2,b1> eb1 e1 f1  f#1 f1 e1 eb1       d1 eb1 e1 f1  f#1 f1 e1 eb1

<d1-,h1-,d1-,g2-> g1 ab1 g1 ab1 g1 ab1    <g1,f#2,g#2> ab1 g1 ab1 g1 ab1 g1 ab1      
<g1,g2> ab1 g1 ab1 g1 ab1 g1 ab1          <g1,f#2,g#2> ab1 g1 ab1 g1 ab1 g1 ab1

<g1,g2> ab1 g1 ab1     <g1,f#2,g#2> ab1 g1 ab1     <g1,g2> ab1    <g1,f#2,g#2> ab1 <a2,g1,f2> ab1  <a#2,g1,e2> ab1  
<h2,d#2,h1,g1> ab1 a1 a#1 h1 a#1 a1 ab1   g1 ab1 a1 a#1 h1 c2 c#2 d2

#teema g:stä
<eb2,g1> f#1 f1 e1 eb1 ab1 g1 f#1    <g1,eb1,c1> f#1 f1 e1 <eb1,c1> e1 f1 f#1
<eb1,g1,c1> f#1 f1 e1 <f1,d1,cb1> e1 eb1 d1    <eb1,c1> e1 f1 f#1 <f1,d1> f#1 g1 g#1
<a1,g1,c#1> g#1 g1 f#1 g1 f#1 f1 e1   f2 e2 eb2 d2 c#2 c2 h1 b1

a1 b1 a1 b1 <a2,a1> b1 <a1,f2> b1     <a1,d2> b1 b1 b1 <a1,d2> b1 <a1,f2> b1
<a2,a1> b1 a1 b1 <a2,a1> b1 <a1,f2> b1     <a1,d2> b1 b1 b1 <a1,d2> b1 <a1,f2> b1

