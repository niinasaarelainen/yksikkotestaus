# kuten alku mutta okt ylempää  
<a2,f1,d1,a1> g#2 g2 f#2 f2 b2 a2 g#2    <a2,f1,d1,a1> g#2 g2 f#2 <f2,d1,h1,g1> f#2 g2 g#2
<a2,f1,d1,a1> g#2 g2 f#2 f2 b2 a2 g2#    <a2,f1,d1,a1> g#2 g2 f#2 <f2,d1,h1,g1> f#2 g2 g#2

<a2,f1,d1,a1> g#2 g2 f#2 <g2,d1,b1,g1> f#2 f2 e2    <f2,d1,a1,f1> f#2 g2 g#2 <a2,g1,c#1,e1> b2 a2 g#2 
<a2,a1,f1,d1> g#2 g2 f#2 <g2,d1,b1,g1> f#2 f2 e2    <f2,d1,a1,f1> f#2 g2 g#2 <a2,g1,c#1,e1> h2 c2 c#2

# nyt eri tavalla
<d2,a1,f1> c#2 c2 h1 <c2,a1,f#1> h1 b1 a1    <b1,g1,d1> a1 g#1 g1 f#1 f1 e1 eb1
<d2,a1,f1> c#2 c2 h1 <c2,a1,f#1> h1 b1 a1    <b1,g1,d1> a1 g#1 g1 f#1 f1 e1 eb1

d1- z- a1 a#1 h1 c2       c#2 d2 d#2 e2 f2 f#2 g2 g#2 
a1 a#1 h1 c2  c#2 d2 d#2 e2     f2 f#1 g1 g#1  a1 h1 c2 c#2
